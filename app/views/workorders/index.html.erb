<p id="notice"><%= notice %></p>

<h1>Workorders</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Workorder number</th>
      <th>Coil size</th>
      <th>Weight</th>
      <th>Number of coils</th>
      <th>Coils delivered</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @workorders.each do |workorder| %>
      <tr>
        <td><%= workorder.date %></td>
        <td><%= workorder.workorder_number %></td>
        <td><%= workorder.coil_size %></td>
        <td><%= workorder.weight %></td>
        <td><%= workorder.number_of_coils %></td>
        <td><%= workorder.coils_delivered %></td>
        <td><%= link_to 'Show', workorder %></td>
        <td><%= link_to 'Edit', edit_workorder_path(workorder) %></td>
        <td><%= link_to 'Destroy', workorder, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Workorder', new_workorder_path %>
